{
  "dotfile": "~/.zshrc",
  "dotfileInject": {
    "template": "source \"{}\"",
    "comment": "# {}",
    "target": "zshrc_postfix.zsh"
  },
  "snippets": {
    "buildTarget": "zshrc_postfix.zsh",
    "pattern": "\\.zsh$"
  },
  "options": {
    "plugins": {
      "directory": "~/.create-my-dotfiles/zsh-plugins",
      "items": {
        "ohmyzsh": {
          "url": "https://github.com/ohmyzsh/ohmyzsh",
          "mainland_url": "https://gitee.com/mirrors/ohmyzsh"
        },
        "zsh-autosuggestions": {
          "url": "https://github.com/zsh-users/zsh-autosuggestions",
          "mainland_url": "https://gitclone.com/github.com/zsh-users/zsh-autosuggestions"
        }
      },
      "zsh-syntax-highlighting": {
        "url": "https://github.com/zsh-users/zsh-syntax-highlighting",
        "mainland_url": "https://gitclone.com/github.com/zsh-users/zsh-syntax-highlighting"
      },
      "zsh-completions": {
        "url": "https://github.com/zsh-users/zsh-completions",
        "mainland_url": "https://gitclone.com/github.com/zsh-users/zsh-completions"
      }
    }
  }
}
