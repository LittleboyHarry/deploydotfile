{
  "dotfile": "~/.zshrc",
  "dotfileInject": {
    "template": "source \"{}\"",
    "target": "zshrc_postfix.zsh"
  },
  "snippets": {
    "buildTarget": "zshrc_postfix.zsh",
    "pattern": "\\.zsh$"
  },
  "options": {
    "plugins": {
      "directory": "~/.zplugins",
      "items": [
        "ohmyzsh/ohmyzsh",
        "zsh-users/zsh-autosuggestions",
        "zsh-users/zsh-syntax-highlighting",
        "zsh-users/zsh-completions"
      ]
    }
  }
}
